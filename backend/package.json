{
  "name": "express-jwt-auth-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for jwt authentication with express in TypeScript.",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
